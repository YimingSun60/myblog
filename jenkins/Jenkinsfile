pipeline {
    agent any

    tools{
        go '1.21'
    }
    environment {
            GO111MODULE = ''
            CGO_ENABLED = 1
            GCCGO='gccgo'
            GOPATH = "${JENKINS_HOME}/jobs/${JOB_NAME}/builds/${BUILD_ID}"
    }

    stages {

        stage('Build') {
            steps {
                sh 'go build -o blog'
            }
        }
    }
}